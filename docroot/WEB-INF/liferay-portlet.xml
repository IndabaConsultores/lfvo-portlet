<?xml version="1.0"?>
<!DOCTYPE liferay-portlet-app PUBLIC "-//Liferay//DTD Portlet Application 6.2.0//EN" "http://www.liferay.com/dtd/liferay-portlet-app_6_2_0.dtd">

<liferay-portlet-app>
	<portlet>
		<portlet-name>lfvo</portlet-name>
		<icon>/icon.png</icon>
		<indexer-class>net.indaba.lostandfound.asset.indexer.ItemIndexer</indexer-class>
		<open-search-class>net.indaba.lostandfound.asset.ItemOpenSearchImpl</open-search-class>
		<control-panel-entry-category>
			site_administration.content
		</control-panel-entry-category>
		<control-panel-entry-weight>3.5</control-panel-entry-weight>
		<asset-renderer-factory>net.indaba.lostandfound.asset.ItemAssetRendererFactory</asset-renderer-factory>
		<header-portlet-css>/css/main.css</header-portlet-css>
		<footer-portlet-javascript>/js/main.js</footer-portlet-javascript>
		<css-class-wrapper>lfvo-portlet</css-class-wrapper>
	</portlet>
	<portlet>
		<portlet-name>app-manager</portlet-name>
		<icon>/icon.png</icon>
		<control-panel-entry-category>
			site_administration.content
		</control-panel-entry-category>
		<control-panel-entry-weight>3.6</control-panel-entry-weight>
		<header-portlet-css>/css/main.css</header-portlet-css>
		<footer-portlet-javascript>
			/js/main.js
		</footer-portlet-javascript>
		<css-class-wrapper>app-manager-portlet</css-class-wrapper>
	</portlet>
	<portlet>
		<portlet-name>office-list</portlet-name>
		<icon>/icon.png</icon>
		<header-portlet-css>/css/main.css</header-portlet-css>
		<footer-portlet-javascript>
			/js/main.js
		</footer-portlet-javascript>
		<css-class-wrapper>office-list-portlet</css-class-wrapper>
	</portlet>
	<portlet>
		<portlet-name>item-list</portlet-name>
		<icon>/icon.png</icon>
		<header-portlet-css>/css/main.css</header-portlet-css>
		<footer-portlet-javascript>
			/js/main.js
		</footer-portlet-javascript>
		<css-class-wrapper>item-list-portlet</css-class-wrapper>
	</portlet>
	<portlet>
		<portlet-name>apk-download</portlet-name>
		<icon>/icon.png</icon>
		<header-portlet-css>/css/main.css</header-portlet-css>
		<footer-portlet-javascript>
			/js/main.js
		</footer-portlet-javascript>
		<css-class-wrapper>apk-download-portlet</css-class-wrapper>
	</portlet>
	<role-mapper>
		<role-name>administrator</role-name>
		<role-link>Administrator</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>guest</role-name>
		<role-link>Guest</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>power-user</role-name>
		<role-link>Power User</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>user</role-name>
		<role-link>User</role-link>
	</role-mapper>
</liferay-portlet-app>